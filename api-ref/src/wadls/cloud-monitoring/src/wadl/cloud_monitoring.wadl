    <method name="GET" id="GetAgentHostInfofilesystems">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Get host filesystem information">
            <para role="shortdesc"> Get information about the filesystems on the host. </para>
        </wadl:doc>
        <response status="200">
            <wadl:doc title="OK" xmlns="http://docbook.org/ns/docbook"><para>The request completed.</para></wadl:doc>
            <representation mediaType="application/xml" xml:lang="EN" element="csapi">
                <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:code href="../docbkx/partials/host_info-filesystems.xml"/>
